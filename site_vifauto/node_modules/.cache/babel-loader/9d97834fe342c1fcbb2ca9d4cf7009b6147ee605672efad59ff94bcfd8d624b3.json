{"ast":null,"code":"import axios from \"axios\";\nimport FournisseurInfoBar from \"@/components/FournisseurInfoBar.vue\";\nimport CardCategorie from \"@/components/CardCategorie.vue\";\nexport default {\n  components: {\n    FournisseurInfoBar,\n    CardCategorie\n  },\n  data() {\n    return {\n      categories: [],\n      fournisseurId: null\n    };\n  },\n  mounted() {\n    this.fournisseurId = parseInt(this.$route.params.id);\n    this.getCategories();\n  },\n  methods: {\n    getCategories() {\n      if (!isNaN(this.fournisseurId)) {\n        axios.get(`http://localhost:3000/api/categories/id=${this.fournisseurId}`).then(response => {\n          this.categories = response.data;\n          console.log(this.categories);\n        }).catch(error => {\n          console.error(\"Error fetching categories:\", error);\n        });\n      } else {\n        console.error(\"Invalid fournisseur ID:\", this.$route.params.id);\n      }\n    }\n  },\n  watch: {\n    $route() {\n      this.fournisseurId = parseInt(this.$route.params.id);\n      this.fetchCategories();\n    }\n  }\n};","map":{"version":3,"names":["axios","FournisseurInfoBar","CardCategorie","components","data","categories","fournisseurId","mounted","parseInt","$route","params","id","getCategories","methods","isNaN","get","then","response","console","log","catch","error","watch","fetchCategories"],"sources":["/Users/jelain/Desktop/Stage VifAuto/GIT/vifauto/site_vifauto/src/views/FournisseurDetailView.vue"],"sourcesContent":["<template>\n  <FournisseurInfoBar />\n  <!--<h1>ID: {{ $route.params.id }}</h1> -->\n\n  <div class=\"fournisseur-detail\">\n    <h1>Categorie</h1>\n    <div class=\"container\">\n      <CardCategorie\n        v-for=\"categorie in categories\"\n        :key=\"categorie.id\"\n        :id=\"categorie.id\"\n        :nom=\"categorie.nom\"\n        :image_url=\"categorie.image_url\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport FournisseurInfoBar from \"@/components/FournisseurInfoBar.vue\";\nimport CardCategorie from \"@/components/CardCategorie.vue\";\nexport default {\n  components: {\n    FournisseurInfoBar,\n    CardCategorie,\n  },\n  data() {\n    return {\n      categories: [],\n      fournisseurId: null,\n    };\n  },\n  mounted() {\n    this.fournisseurId = parseInt(this.$route.params.id);\n    this.getCategories();\n  },\n  methods: {\n    getCategories() {\n      if (!isNaN(this.fournisseurId)) {\n        axios\n          .get(`http://localhost:3000/api/categories/id=${this.fournisseurId}`)\n          .then((response) => {\n            this.categories = response.data;\n            console.log(this.categories);\n          })\n          .catch((error) => {\n            console.error(\"Error fetching categories:\", error);\n          });\n      } else {\n        console.error(\"Invalid fournisseur ID:\", this.$route.params.id);\n      }\n    },\n  },\n  watch: {\n    $route() {\n      this.fournisseurId = parseInt(this.$route.params.id);\n      this.fetchCategories();\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.fournisseur-detail {\n  width: 60em;\n  margin: 20px 0;\n  h1 {\n    font-weight: 600;\n    margin: 30px 0;\n  }\n  .container {\n    display: grid;\n    grid-template-columns: repeat(5, 1fr);\n    gap: 20px;\n  }\n}\n</style>\n"],"mappings":"AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,kBAAiB,MAAO,qCAAqC;AACpE,OAAOC,aAAY,MAAO,gCAAgC;AAC1D,eAAe;EACbC,UAAU,EAAE;IACVF,kBAAkB;IAClBC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACD,aAAY,GAAIE,QAAQ,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;IACpD,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPD,aAAaA,CAAA,EAAG;MACd,IAAI,CAACE,KAAK,CAAC,IAAI,CAACR,aAAa,CAAC,EAAE;QAC9BN,KAAI,CACDe,GAAG,CAAE,2CAA0C,IAAI,CAACT,aAAc,EAAC,EACnEU,IAAI,CAAEC,QAAQ,IAAK;UAClB,IAAI,CAACZ,UAAS,GAAIY,QAAQ,CAACb,IAAI;UAC/Bc,OAAO,CAACC,GAAG,CAAC,IAAI,CAACd,UAAU,CAAC;QAC9B